<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Access Control</title>
</head>
<body>

<h2>Registros<h2>
    <table border="1">
        <thead>
            <tr> 
                <td>Numero da Sala</td>  
                <td>Horario</td>  
                <td>Descricao</td> 
                <td>Responsavel</td> 
                <td> Estado </td>  
                <td> Datas </td> 
                
            </tr>
        </thead>
        <tbody>
        <% data.forEach(function(details) { %>
            <tr>
                <td><%= details.numero %></td> 
                <td><%= details.horario %></td>
                <td><%= details.descricao %></td> 
                <td><%= details.responsavel %></td> 
                <td><%= details.stado %></td> 
                <td><%= details.datas %></td>
                <td><a href="/edit/<%= details._id %>"> Editar </a> - <a href="/delete/<%= details._id %>"> Deletar </a> </td>
            </tr>
            <% }) %>
        </tbody>
        <button><a href="/">Voltar</a></button>
</body>
</html>
